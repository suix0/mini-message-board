<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Message Board</title>
    <link rel="stylesheet" href="styles/styles.css" />
  </head>
  <body>
    <div class="container">
      <button>Send a Message!</button>
      <div>
        <h1>Messages</h1>
        <% messages.forEach(message => { %> <%- include('messageCard', {message:
        message.message, to: message.to, added: message.added}) %> <% }) %>
      </div>
    </div>
  </body>
</html>
